//         浮点数测试
//#include<stdio.h>
//int main()
//{
//    float i = 0;
//    scanf("%f", &i);
//    printf("%.3f", i);
//    return 0;
//}
#include<stdio.h>
int main()
{
    int i[3] = { 0 };
    int y = 0;
    for (y = 0; y < 3; y++)
    {
        scanf("%d", i[y]);
    }
    printf("%d", i[1]);
    return 0;
}
