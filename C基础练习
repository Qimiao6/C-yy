//            写一个求k的n次方函数
#include <math.h >
double pp(double x, double y)
{
    return pow(x, y);//pow是用来运算次方的
}
#include <stdio.h>
int main()
{
    int i = 2;
    int u = 0;
    printf("%lf", pp(i, u));
    return 0;
}
